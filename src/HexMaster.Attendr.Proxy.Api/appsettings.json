{
  "ReverseProxy": {
    "Routes": {
      "profiles-route": {
        "ClusterId": "profiles-cluster",
        "Match": {
          "Path": "/profiles/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "/api/{**catch-all}"
          }
        ]
      },
      "groups-route": {
        "ClusterId": "groups-cluster",
        "Match": {
          "Path": "/groups/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "/api/{**catch-all}"
          }
        ]
      },
      "conferences-route": {
        "ClusterId": "conferences-cluster",
        "Match": {
          "Path": "/conferences/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "/api/{**catch-all}"
          }
        ]
      }
    },
    "Clusters": {
      "profiles-cluster": {
        "Destinations": {
          "profiles": {
            "Address": "https://localhost:7001"
          }
        }
      },
      "groups-cluster": {
        "Destinations": {
          "groups": {
            "Address": "https://localhost:7002"
          }
        }
      },
      "conferences-cluster": {
        "Destinations": {
          "conferences": {
            "Address": "https://localhost:7003"
          }
        }
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}